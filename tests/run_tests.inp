assert_not_equal_str(" ABC", "ABC")
catch assert_not_equal_str(toupper("abc"), "ABC")
if !$error
    stop
endif
catch assert_not_equal_str("", "")
if !$error
    stop
endif
assert_not_equal_str("", "__/%(=O?")
catch assert_not_equal_str("739838", "739838")
if !$error
    stop
endif



